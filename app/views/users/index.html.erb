<%= render 'list', book: @book, user: @user %>
  <div class="col-md-8 offset-md-1">
  <h2>Users</h2>
  <table class="table">
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td>
          <a href="/users/<%= user.id %>">
            <% if user.profile_image.attached? %>
              <%= image_tag user.profile_image.variant(resize_to_limit: [75, 75]), alt: "User Avatar" %>
            <% else %>
              <%= image_tag "default-image.jpg", size: "75x75", alt: "Default Avatar" %>
            <% end %>
          </a>
          </td>
          <td><%= user.name %></td>
          <td><a href="/users/<%= user.id %>">Show</a></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
    </div>
  </div>
</main>